<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter type="string" key="false">0</parameter>
    </parameters>
    <services>

        <service id="Mollie\Shopware\Component\Settings\SettingsService">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="string">%env(default:false:MOLLIE_DEV_MODE)%</argument>
            <argument type="string">%env(default:false:MOLLIE_CYPRESS_MODE)%</argument>
        </service>


    </services>
</container>