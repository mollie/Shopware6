<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Mollie\Shopware\Component\FlowBuilder\Storer\PaymentDataStorer">
            <tag name="flow.storer"/>
        </service>

        <service id="Mollie\Shopware\Component\FlowBuilder\Subscriber\BusinessEventSubscriber">
            <argument type="service" id="Shopware\Core\Framework\Event\BusinessEventCollector"/>
            <argument type="service" id="Mollie\Shopware\Component\FlowBuilder\WebhookStatusEventFactory"/>
            <tag name="kernel.event_subscriber"/>
        </service>
        <service id="Mollie\Shopware\Component\FlowBuilder\WebhookStatusEventFactory"/>

    </services>
</container>