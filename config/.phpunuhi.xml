<phpunuhi
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/boxblinkracer/phpunuhi/refs/heads/main/schema/dev-main.xsd"
>
    <translations>

        <set name="administration">
            <format>
                <json indent="2"/>
            </format>
            <locales>
                <locale name="de-DE">./../src/Resources/app/administration/src/snippet/de-DE.json</locale>
                <locale name="en-GB">./../src/Resources/app/administration/src/snippet/en-GB.json</locale>
                <locale name="nl-NL">./../src/Resources/app/administration/src/snippet/nl-NL.json</locale>
                <locale name="it-IT">./../src/Resources/app/administration/src/snippet/it-IT.json</locale>
                <locale name="pt-PT">./../src/Resources/app/administration/src/snippet/pt-PT.json</locale>
                <locale name="es-ES">./../src/Resources/app/administration/src/snippet/es-ES.json</locale>
                <locale name="sv-SE">./../src/Resources/app/administration/src/snippet/sv-SE.json</locale>
                <locale name="nb-NO">./../src/Resources/app/administration/src/snippet/nb-NO.json</locale>
                <locale name="pl-PL">./../src/Resources/app/administration/src/snippet/pl-PL.json</locale>
                <locale name="cs-CZ">./../src/Resources/app/administration/src/snippet/cs-CZ.json</locale>
                <locale name="sl-SL">./../src/Resources/app/administration/src/snippet/sl-SL.json</locale>
                <locale name="hu-HU">./../src/Resources/app/administration/src/snippet/hu-HU.json</locale>
            </locales>
        </set>

        <set name="storefront">
            <format>
                <json indent="2"/>
            </format>
            <styles>
                <style>camel</style>
                <style>kebab</style>
            </styles>
            <locales>
                <locale name="de-DE">./../src/Resources/snippet/de_DE/mollie-payments.de-DE.json</locale>
                <locale name="en-GB">./../src/Resources/snippet/en_GB/mollie-payments.en-GB.json</locale>
                <locale name="nl-NL">./../src/Resources/snippet/nl_NL/mollie-payments.nl-NL.json</locale>
                <locale name="it-IT">./../src/Resources/snippet/it_IT/mollie-payments.it-IT.json</locale>
                <locale name="pt-PT">./../src/Resources/snippet/pt_PT/mollie-payments.pt-PT.json</locale>
                <locale name="es-ES">./../src/Resources/snippet/es_ES/mollie-payments.es-ES.json</locale>
                <locale name="sv-SE">./../src/Resources/snippet/sv_SE/mollie-payments.sv-SE.json</locale>
                <locale name="nb-NO">./../src/Resources/snippet/nb_NO/mollie-payments.nb-NO.json</locale>
                <locale name="pl-PL">./../src/Resources/snippet/pl_PL/mollie-payments.pl-PL.json</locale>
                <locale name="cs-CZ">./../src/Resources/snippet/cs_CZ/mollie-payments.cs-CZ.json</locale>
                <locale name="sl-SL">./../src/Resources/snippet/sl_SL/mollie-payments.sl-SL.json</locale>
                <locale name="hu-HU">./../src/Resources/snippet/hu_HU/mollie-payments.hu-HU.json</locale>
            </locales>
        </set>

        <set name="plugin-settings">
            <rules>
                <emptyContent>
                    <key name="card.0.title">
                        <locale>*</locale>
                    </key>
                </emptyContent>
            </rules>
            <format>
                <shopware6 type="config" file="../src/Resources/config/config.xml"/>
            </format>
            <locales>
                <locale name="en-GB" base="true"></locale>
                <locale name="de-DE"></locale>
                <locale name="nl-NL"></locale>
                <locale name="it-IT"></locale>
                <locale name="pt-PT"></locale>
                <locale name="es-ES"></locale>
                <locale name="sv-SE"></locale>
                <locale name="nb-NO"></locale>
                <locale name="pl-PL"></locale>
                <locale name="cs-CZ"></locale>
                <locale name="sl-SL"></locale>
                <locale name="hu-HU"></locale>
            </locales>
        </set>

    </translations>

</phpunuhi>