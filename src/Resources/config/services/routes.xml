<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Kiener\MolliePayments\Controller\Routes\ApplePayDirect\IsEnabledRoute" public="true">
            <argument type="service" id="Kiener\MolliePayments\Service\SettingsService"/>
            <argument type="service" id="Kiener\MolliePayments\Repository\PaymentMethod\PaymentMethodRepository"/>
        </service>

        <service id="Kiener\MolliePayments\Controller\Routes\ApplePayDirect\PaymentIdRoute" public="true">
            <argument type="service" id="Kiener\MolliePayments\Repository\PaymentMethod\PaymentMethodRepository"/>
        </service>

        <service id="Kiener\MolliePayments\Controller\Routes\ApplePayDirect\AddProductRoute" public="true">
            <argument type="service" id="Kiener\MolliePayments\Service\CartService"/>
            <argument type="service" id="Kiener\MolliePayments\Service\Cart\CartBackupService"/>
        </service>

        <service id="Kiener\MolliePayments\Controller\Routes\ApplePayDirect\CreateSessionRoute" public="true">
            <argument type="service" id="Kiener\MolliePayments\Factory\MollieApiFactory"/>
            <argument type="service" id="Kiener\MolliePayments\Service\ShopService"/>
        </service>

        <service id="Kiener\MolliePayments\Controller\Routes\ApplePayDirect\ShippingMethodRoute" public="true">
            <argument type="service" id="Kiener\MolliePayments\Service\SettingsService"/>
            <argument type="service" id="Kiener\MolliePayments\Service\CartService"/>
            <argument type="service" id="Kiener\MolliePayments\Service\ApplePayDirect\ApplePayDirect"/>
            <argument type="service" id="Kiener\MolliePayments\Service\CustomerService"/>
        </service>

    </services>
</container>
