<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Kiener\MolliePayments\Components\Subscription\DAL\Subscription\SubscriptionDefinition">
            <tag name="shopware.entity.definition" entity="mollie_subscription_to_product"/>
        </service>

        <service id="Kiener\MolliePayments\Components\Subscription\Builder\MollieDataBuilder">
            <argument type="service" id="Symfony\Component\Routing\RouterInterface"/>
        </service>

        <service id="Kiener\MolliePayments\Components\Subscription\Builder\SubscriptionBuilder">
        </service>

        <service id="Kiener\MolliePayments\Components\Subscription\Cart\PaymentMethodAvailability\PaymentMethodAvailabilityValidator">
            <argument type="service" id="Kiener\MolliePayments\Service\SettingsService"/>
            <tag name="shopware.cart.validator"/>
        </service>

        <service id="Kiener\MolliePayments\Components\Subscription\Page\Account\Mollie\AccountSubscriptionsPageLoader">
            <argument type="service" id="mollie_subscription.repository"/>
            <argument type="service" id="Shopware\Storefront\Page\GenericPageLoader"/>
            <argument type="service" id="Kiener\MolliePayments\Service\CustomerService"/>
        </service>

        <service id="Kiener\MolliePayments\Components\Subscription\SubscriptionManager">
            <argument type="service" id="mollie_subscription.repository"/>
            <argument type="service" id="Kiener\MolliePayments\Components\Subscription\Builder\MollieDataBuilder"/>
            <argument type="service" id="Kiener\MolliePayments\Components\Subscription\Builder\SubscriptionBuilder"/>
            <argument type="service" id="Kiener\MolliePayments\Service\CustomerService"/>
            <argument type="service" id="Kiener\MolliePayments\Gateway\Mollie\MollieGateway"/>
            <argument type="service" id="mollie_payments.logger"/>
        </service>

    </services>
</container>
