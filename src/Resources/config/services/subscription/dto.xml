<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Kiener\MolliePayments\Service\Subscription\DTO\SubscriptionOption"
                 class="Kiener\MolliePayments\Service\Subscription\DTO\SubscriptionOption">
            <argument type="service" id="Shopware\Core\Checkout\Payment\SalesChannel\PaymentMethodRoute"/>
        </service>

        <service id="Kiener\MolliePayments\Service\Subscription\DTO\SubscriptionResponse"
                 class="Kiener\MolliePayments\Service\Subscription\DTO\SubscriptionResponse">
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="Shopware\Core\System\SalesChannel\Context\SalesChannelContextService"/>
        </service>
    </services>
</container>