<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Kiener\MolliePayments\Repository\CustomFieldSet\CustomFieldSetRepository"
                 class="Kiener\MolliePayments\Repository\CustomFieldSet\CustomFieldSetRepository">
            <argument type="service" id="custom_field_set.repository"/>
        </service>


        <service id="Kiener\MolliePayments\Repository\Order\OrderAddressRepository"
                 class="Kiener\MolliePayments\Repository\Order\OrderAddressRepository">
            <argument type="service" id="order_address.repository"/>
        </service>

        <service id="Kiener\MolliePayments\Repository\Order\OrderRepository"
                 class="Kiener\MolliePayments\Repository\Order\OrderRepository">
            <argument type="service" id="order.repository"/>
        </service>

        <service id="Kiener\MolliePayments\Repository\Product\ProductRepository"
                 class="Kiener\MolliePayments\Repository\Product\ProductRepository">
            <argument type="service" id="product.repository"/>
        </service>

        <service id="Kiener\MolliePayments\Repository\PaymentMethod\PaymentMethodRepository">
            <argument type="service" id="payment_method.repository"/>
        </service>

        <service id="Kiener\MolliePayments\Repository\MollieSubscriptionHistory\MollieSubscriptionHistoryRepository">
            <argument type="service" id="mollie_subscription_history.repository"/>
        </service>

    </services>
</container>
