<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        https://symfony.com/schema/routing/routing-1.0.xsd">


    <route id="store-api.mollie.config"
           path="/store-api/mollie/config"
           methods="GET">
        <default key="_controller">Kiener\MolliePayments\Controller\StoreApi\Config\ConfigControllerBase::getConfig</default>
        <default key="_routeScope"><list><string>store-api</string></list></default>
    </route>
</routes>
