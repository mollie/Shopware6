<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        https://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="frontend.mollie.ideal.storeIssuer" path="/mollie/ideal/store-issuer/{customerId}/{issuerId}" methods="GET">
        <default key="_controller">Kiener\MolliePayments\Controller\Storefront\iDEAL\iDealControllerBase::storeIssuer</default>
        <default key="_routeScope"><list><string>storefront</string></list></default>
        <option key="seo">false</option>
    </route>

</routes>
