parameters:
    level: 1
    paths:
        - ./src
    excludes_analyse:
        - ./src/Resources/app/administration/node_modules/*
        - ./src/Resources/app/storefront/node_modules/*
        # already in phpstan lvl8
        - ./src/Gateway

services:
    - class: MolliePayments\PHPStan\Rules\NoManufacturerRule
      tags:
        - phpstan.rules.rule
